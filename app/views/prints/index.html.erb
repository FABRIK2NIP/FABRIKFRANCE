<div style="float:right; display:inline; margin-top:-20px; margin-right:-390px;width:28px; height:18px;position:absolute; width:120px;">
	
	
	
	
		<div id="rangement" style="position:absolute; right:0; width:120px; height:27px;border-radius:18px; border:1px solid #454CDC;float:right">
			<div id="rrond" style="width:15px; height:15px; background-color:#FE9321; border-radius:80%; 			float:right;margin-top:5px;margin-right:7px;opacity:1.5;"></div>
			<h5 id="texteetat" style="margin-top:5px; margin-bottom:0px;margin-left:0px; margin-right:8px; float:right;font-size:13px;letter-spacing:0px;font-weight:300;color:#787878">ready to print</h5>
		</div>
	
</div>
</div>






	<script>
	
	
	/* Setup variables */
		
		   var tl70;

		   // Controller
		   var controller70 = new ScrollMagic();


	   /* Create a timeline */
	   tl70 = new TimelineMax();
	   
	   

	  
	 
	   tl70.set("#rangement",  { width:30, borderColor:'#ffffff'});
	   tl70.set("#texteetat", { opacity:0});
	   tl70.set("#texteetat",{text:"Loading"});
	   tl70.set("#texteetat",{text:"Ready to print"}, 6.5);
		 
  	 
	   
		 tl70.to("#rangement", 1.5, { borderColor:'#787878',  ease:Power4.easeOut}, 1.9)
	         .to("#rrond", 0.8, { backgroundColor:'#FE9321',  ease:Power4.easeOut}, 1)
	         .to("#rrond", 0.8, { backgroundColor:'#F60109',  ease:Power4.easeOut}, 1.8)
	         .to("#rrond", 0.8, { backgroundColor:'#FE9321',  ease:Power4.easeOut}, 2.6)
	         .to("#rrond", 0.8, { backgroundColor:'#F60109',  ease:Power4.easeOut}, 3.4)
	         .to("#rrond", 0.8, { backgroundColor:'#FE9321',  ease:Power4.easeOut}, 4.2)
	         .to("#rrond", 0.8, { backgroundColor:'#F60109',  ease:Power4.easeOut}, 5)
	         .to("#rrond", 0.8, { backgroundColor:'#FE9321',  ease:Power4.easeOut}, 5.8)
	         .to("#rrond", 0.8, { backgroundColor:'#F60109',  ease:Power4.easeOut}, 6.6)
	         .to("#rrond", 2, { backgroundColor:'#00AD43',  ease:Power4.easeOut}, 7.4)
	   
		     .to("#rangement", 1.5, { width:90,  ease:Power4.easeOut}, 2)
	         .to("#texteetat", 2, { opacity:1, ease:Power4.easeOut},2.5)
		     .to("#texteetat", 0.2, { opacity:0, ease:Power4.easeOut}, 5)
		     .to("#rangement", 1.5, { width:30,  ease:Power4.easeOut}, 5)
	         .to("#rangement", 1, {borderColor:'#ffffff',  ease:Power4.easeOut}, 5.6)
	      
	         .to("#rangement", 1.5, { borderColor:'#787878',  ease:Power4.easeOut}, 6.4)
		     .to("#rangement", 1.5, { width:120,  ease:Power4.easeOut}, 6.6)
	         .to("#texteetat", 2, { opacity:1, ease:Power4.easeOut},7.6)
		     .to("#texteetat", 0.2, { opacity:0, ease:Power4.easeOut}, 10.6)
		     .to("#rangement", 1.5, { width:30,  ease:Power4.easeOut}, 10.6)
	         .to("#rangement", 1, {borderColor:'#ffffff',  ease:Power4.easeOut}, 11.6)
			 
	   .add("end");
	   
	   
	   
	   
	   
	       
	       
	   
	   
	   var scene = new ScrollScene({triggerElement: "#Calque_8"})
	       .addTo(controller70)
	       .setTween(tl70);
	   
	   
		   scene.addIndicators();
	 	  	
		   scene.reverse(false);
		   
	 	 
 
	</script>
	






