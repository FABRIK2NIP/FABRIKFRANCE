<%= form_for @print, method: :put, url: wizard_path do |f| %>
  <% if f.object.errors.any? %>
    <div class="error_messages">
      <% f.object.errors.full_messages.each do |error| %>
        <p><%= error %></p>
      <% end %>
    </div>
  <% end %>

<%= content_tag :div, class: "print_attachment", data: {print: @print} do %>
<% end %>


